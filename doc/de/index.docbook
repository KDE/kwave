	<simplesect
><title
>Siehe auch</title>
	    <para>
		<link linkend="cmd_sect_close"
><command
>&no-i18n-tag;&no-i18n-cmd_close;</command
>()</link>
	    </para>
	</simplesect>
	</sect2>
    </sect1>

