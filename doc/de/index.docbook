		<imageobject>
		    <imagedata fileref="kwave-plugin-fileinfo.png" format="PNG"/>
		</imageobject>
		<textobject>
		    <phrase
>Screenshot des Dialogs mit Datei-Informationen</phrase>
		</textobject>
	    </mediaobject>
	</screenshot>
    </para>

    </sect1>

