					    <row>
						<entry
>&no-i18n-tag;<command
>1</command
></entry>
						<entry
>amostras</entry>
					    </row>
					    <row>
						<entry
>&no-i18n-tag;<command
>2</command
></entry>
						<entry
>porcentagem do tamanho do arquivo</entry>
